<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.CommonQAMapper">
	<select id="list" resultType="com.example.domain.CommonQAVO">
		select * from tbl_commonQA order by commonQA_number desc limit #{pageStart}, #{perPageNum}
	</select>
	<select id="read" resultType="com.example.domain.CommonQAVO">
		select * from tbl_commonQA where commonQA_number=#{commonQA_number}
	</select>
	
</mapper>